var $entries = $(".entries");

$entries.find('.entry-li.next-entries')
        .replaceWith("<%=j entry_list(@entries, show_list_end: true) %>");


$entries.find("<%=j @entries.map{|e| "[data-entry-id='#{e.id}']"  }.join(',').html_safe %>")
        .each(function(){
          initializeEntry( $(this) );
        });
