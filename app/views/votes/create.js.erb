var $entry   = $(".entry[data-schemable-id='<%= @entry.schemable_id %>']" +
                       "[data-schemable-type='<%= @entry.schemable_type %>']"),
    $actions = $entry.find('.actions > .actions-left');

$actions.children('.action.vote-action').remove();
$actions.prepend("<%=j render 'entries/vote_links', entry: @entry, reload_votes: true %>");